|#

(require 'bigint)
(define (fob n)
	(if (< n 2) n (+ (fob (1- n)) (fob (- n 2)))))
	
(remember 'fob)

;; tail recursive
(define (fib n)
  (let loop ((a 0) (b 1) (n n))
    (if (= n 0) a
        (loop b (+ a b) (- n 1)))))
        
